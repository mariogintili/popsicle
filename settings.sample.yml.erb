popsicle:
  app_name: <%= ENV["POPSICLE_APP_NAME"] || "popsicle_application" %>
  index_key: <%= ENV["POPSICLE_INDEX_KEY"] || "revision" %>
  headers:
    Accept: "text/html"
  store:
    redis:
      host: <%= ENV["POPSICLE_REDIS_HOST"] || "localhost" %>
      port: <%= ENV["POPSICLE_REDIS_PORT"] || 6379 %>
      password: <% ENV["POPSICLE_REDIS_PASSWORD"] %>
      timeout: <% ENV["POPSICLE_REDIS_TIMEOUT"] || 1000 %>
      reconnect_attempts: <% ENV["POPSICLE_REDIS_RECONNECT_ATTEMPTS"] || 3 %>
      role: <% ENV["POPSICLE_REDIS_ROLE"] || "master" %>
